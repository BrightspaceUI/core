{
  "name": "d2l-core-ui",
  "version": "0.0.1",
  "description": "Brightspace core UI components.",
  "repository": "https://github.com/BrightspaceUI/core.git",
  "main": "index.html",
  "scripts": {
    "build:icons": "node ./cli/iconBuilder.js",
    "lint": "eslint . --ext .js,.html",
    "postinstall": "npm run build:icons",
    "test": "npm run lint && npm run test:local && npm run test:diff",
    "test:diff": "mocha './test/**/*.visual-diff.js' -t 10000",
    "test:diff:golden": "mocha './test/**/*.visual-diff.js' -t 10000 --golden",
    "test:local": "polymer test --skip-plugin sauce",
    "test:sauce": "polymer test --skip-plugin local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polymer/test-fixture": "latest",
    "axe-core": "latest",
    "babel-eslint": "latest",
    "chai": "latest",
    "demo-transfigurator": "dbatiste/demo-transfigurator",
    "eslint": "latest",
    "eslint-config-brightspace": "latest",
    "eslint-plugin-html": "latest",
    "eslint-plugin-lit": "latest",
    "mocha": "^5.0.0",
    "polymer-cli": "latest",
    "puppeteer": "latest",
    "visual-diff": "dbatiste/visual-diff",
    "wct-mocha": "^1.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@webcomponents/shadycss": "latest",
    "@webcomponents/webcomponentsjs": "latest",
    "d2l-icons": "BrightspaceUI/icons#semver:^6",
    "d2l-intl": "^2.0.0",
    "fastdom": "^1.0.9",
    "intl-messageformat": "^2.2.0",
    "lit-element": "latest",
    "resize-observer-polyfill": "^1.5.1"
  },
  "greenkeeper": {
    "ignore": [
      "mocha"
    ]
  }
}
